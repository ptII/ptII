		in the same order as given here. -->
	<property name="droid_tabs">
		<!-- Example tab 1 -->
		<property name="tab1" class="ptolemy.kernel.util.StringAttribute"
			value="First tab">
			<!-- Label example. -->
			<property name="label1" class="ptolemy.kernel.util.StringAttribute"
				value="Example label">
