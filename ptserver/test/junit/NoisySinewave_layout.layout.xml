				<!-- AndroidLocation is a currently non-existing class. It will contain 
					the following information: The x and y position of the top-left corner The 
					height and width of the draw area. -->
				<property name="droid_location" class="ptserver.test.helper.AndroidLocation"
					value="[10, 15, -2, -2]"></property>
				<!-- <property name="droid_typeface" class="ptolemy.data.expr.Parameter" 
