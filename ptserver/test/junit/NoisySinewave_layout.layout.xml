			<property name="standardDeviation" class="ptolemy.actor.parameters.PortParameter"
				value="noiseStandardDeviation">
			</property>
			<doc>Create a sequence of gaussian random variables</doc>
			<property name="_location" class="ptolemy.kernel.util.Location"
				value="310.0, 345.0">
			</property>
		</entity>
		<relation name="relation5" class="ptolemy.actor.TypedIORelation">
		</relation>
		<relation name="relation7" class="ptolemy.actor.TypedIORelation">
		</relation>
		<link port="noisy" relation="relation7" />
		<link port="AddSubtract2.plus" relation="relation5" />
		<link port="AddSubtract2.plus" relation="relation3" />
		<link port="AddSubtract2.output" relation="relation7" />
		<link port="Gaussian.output" relation="relation5" />
	</class>
