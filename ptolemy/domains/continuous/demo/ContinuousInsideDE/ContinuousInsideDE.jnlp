<?xml version="1.0" encoding="UTF-8"?>
<!-- JNLP is the Java Network Launching Protocol
     This file is derived from a *jnlp.in file by running
     sed on the *.jnlp.in file and substituting 
     file:/Users/cxh/ptII/
-->
<!-- href is needed so that web start can cache the app and
     it will appear in the desktop icon
-->
<jnlp codebase="file:/Users/cxh/ptII/"
      href="file:/Users/cxh/ptII/ptolemy/domains/continuous/demo/ContinuousInsideDE/ContinuousInsideDE.jnlp"> 
<!-- jnlp
      codebase="file:./../../../../.." 
      href="ptolemy/domains/continuous/demo/ContinuousInsideDE/ContinuousInsideDE.jnlp"-->
