<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">

<group>
  
  <entity name="BaseDimension" class="ptolemy.data.ontologies.lattice.unit.BaseDimensionRepresentativeConcept">
    <property name="solutionColor" class="ptolemy.actor.gui.ColorAttribute" value="{1.0,0.0,0.8,1.0}"/>
    <property name="LatticeElementIcon" class="ptolemy.vergil.ontologies.MultipleConceptIcon"/>
  </entity>
  <entity name="DerivedDimension" class="ptolemy.data.ontologies.lattice.unit.DerivedDimensionRepresentativeConcept">
    <property name="solutionColor" class="ptolemy.actor.gui.ColorAttribute" value="{1.0,0.4,0.0,1.0}"/>
    <property name="LatticeElementIcon" class="ptolemy.vergil.ontologies.MultipleConceptIcon"/>
  </entity>
  
  <!-- Pre-Defined Base Dimension Concepts library. -->
  <entity name="Pre-Defined Base Dimensions" class="ptolemy.moml.EntityLibrary">
    <configure>
      <?moml
        <group>
          <input source="ptolemy/data/ontologies/lattice/unit/baseDimensionsLibrary.xml"/>
        </group>
      ?>
    </configure>
  </entity>
  
  <entity name="Pre-Defined Derived Dimensions" class="ptolemy.moml.EntityLibrary">
    <configure>
      <?moml
        <group>
          <input source="ptolemy/data/ontologies/lattice/unit/derivedDimensionsLibrary.xml"/>
        </group>
      ?>
    </configure>
  </entity>

</group>
