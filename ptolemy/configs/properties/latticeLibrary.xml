<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">

<group>
  <input source="ptolemy/configs/annotation.xml"/>
  
  <entity name="Top" class="ptolemy.domains.properties.LatticeElement">
    <property name="LatticeElementIcon" class="ptolemy.vergil.properties.LatticeElementIcon"/>
  </entity>
  <entity name="Bottom" class="ptolemy.domains.properties.LatticeElement">
    <property name="isAcceptableSolution" class="ptolemy.data.expr.Parameter" value="false"/>
    <property name="LatticeElementIcon" class="ptolemy.vergil.properties.LatticeElementIcon"/>
  </entity>

  <entity name="element" class="ptolemy.domains.properties.LatticeElement">
    <property name="solutionColor" class="ptolemy.actor.gui.ColorAttribute" value="{0.2,1.0,0.4,1.0}"/>
    <property name="LatticeElementIcon" class="ptolemy.vergil.properties.LatticeElementIcon"/>
  </entity>

</group>
