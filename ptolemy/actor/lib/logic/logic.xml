<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="Logic" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
  <doc>Logic functions</doc>

  <entity name="Comparator" class="ptolemy.actor.lib.logic.Comparator">
    <property name="comparison">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="gt" value="&gt;"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="ge" value="&gt;="
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="lt" value="&lt;"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="le" value="&lt;="
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="eq" value="=="
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
    <property name="_icon" class="ptolemy.vergil.icon.AttributeValueIcon">
        <property name="attributeName" value="comparison"/>
    </property>
  </entity>

<entity name="ErrorAction" class="ptolemy.actor.lib.Error">
    <property name="errorAction">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="warn" value="Warn"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="reset" value="Reset"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="timeutility" value="TimedUtilityFucntion"
	              class="ptolemy.kernel.util.StringAttribute"/>
            </property>
     </property>
    <property name="_icon" class="ptolemy.vergil.icon.AttributeValueIcon">
        <property name="attributeName" value="ErrorAction"/>
    </property>
  </entity>

  <entity name="Equals" class="ptolemy.actor.lib.logic.Equals">
  </entity>

  <entity name="IsPresent" class="ptolemy.actor.lib.logic.IsPresent">
  </entity>

  <entity name="LogicalNot" class="ptolemy.actor.lib.logic.LogicalNot">
  </entity>

  <entity name="LogicFunction" class="ptolemy.actor.lib.logic.LogicFunction">
    <property name="function">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="and" value="and"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="or" value="or"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="xor" value="xor"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="nand" value="nand"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="nor" value="nor"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="xnor" value="xnor"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
    <property name="_icon" class="ptolemy.vergil.icon.AttributeValueIcon">
        <property name="attributeName" value="function"/>
    </property>
  </entity>

      </group>
    ?>
  </configure>
</entity>


