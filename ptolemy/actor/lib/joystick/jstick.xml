<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">


<!-- This file is named jstick.xml so as to avoid a StackOverflowError
     in PtolemyEffigy that occurs if the Joystick class cannot be loaded
     and there is a joystick.xml file present
-->

<entity name="Joystick" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
<doc>Joystick Actors</doc>

<entity name="Joystick"
        class="ptolemy.actor.lib.joystick.Joystick">
</entity>

      </group>
    ?>
  </configure>
</entity>
