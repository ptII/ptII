        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="{105, 235}">
        </property>
    </entity>
    <entity name="Test" class="ptolemy.actor.lib.Test">
        <property name="correctValues" class="ptolemy.data.expr.Parameter" value="{&quot;value= foo&quot;, &quot;value= foo&quot;, &quot;value= foo&quot;}">
        </property>
        <property name="trainingMode" class="ptolemy.actor.parameters.SharedParameter" value="false">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="{245, 255}">
        </property>
    </entity>
    <relation name="relation" class="ptolemy.actor.TypedIORelation">
        <property name="width" class="ptolemy.data.expr.Parameter" value="1">
        </property>
    </relation>
    <link port="Expression.output" relation="relation"/>
    <link port="Test.input" relation="relation"/>
</entity>
