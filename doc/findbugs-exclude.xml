<FindBugsFilter>
    <!-- ApplianceTest etc. are part of the x10 sources in vendors/ -->
    <Match classregex="ApplianceTest"/>
    <Match classregex="LightTest"/>
    <Match classregex="MasterTest"/>
    <Match classregex="X10SerialServer"/>
    <Match classregex="X10SerialServerTest"/>

    <Match classregex="ptolemy.data..*Parser.*"/>
    <Match classregex="ptolemy.data.unit.UParser"/>
    <Match classregex="ptolemy.data.unit.UParserTokenManager"/>
    <Match classregex="com.jgoodies.*"/>
    <Match classregex="com.izforge.*"/>
    <Match classregex="com.centralnexus.input.*"/>
    <Match classregex="com.coi.tools.os.win.*"/>
    <Match classregex="net.tinyos.*"/>
    <Match classregex=".*" >
       <!-- Exclude Serializable warnings -->
       <!-- Exclude class is Serializable; consider declaring a serialVersionUID -->
       <BugCode name="Se,SnVI" />
    </Match>
</FindBugsFilter>
